{
  "name": "@embley/google-secrets-manager-cloudflare",
    "version": "1.0.25",
  "files": [
    "build/*"
  ],
  "description": "Google Secrets Manager - Cloudflare",
  "keywords": [
    "google secrets manager",
    "cloudflare",
    "edge"
  ],
  "main": "./build/index.js",
  "scripts": {
    "start": "node src/index.ts",
    "build": "tsc",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts --fix --max-warnings=0"
  },
  "author": "Rafael Cuevas & Eusebio Trigo",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkventurestudio/gcsm-cloudflare"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "@types/node": "20.14.9",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "9.6.0",
    "ts-node": "10.9.2",
    "tsup": "8.1.0",
    "vitest": "1.6.0",
    "typescript": "5.5.2"
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  }
}
